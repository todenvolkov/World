				<Id>172</Id>
				<Title>Street Party on Rockford Ct. </Title>
				<BitrixTags></BitrixTags>
				<PropertiesValues>
					<PropertyValues>
						<Id>CML2_ACTIVE</Id>
						<Value>true</Value>
					</PropertyValues>
					<PropertyValues>
						<Id>CML2_CODE</Id>
